@model LeaveSystemMVC.Models.sLeaveModel
@Html.Partial("_LeftMenu")
<div class="col-md-6">
    @using (Html.BeginForm()) 
    {
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Line Manager Subordinate History Page</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        
           
                   <div class="col-md-6">
                       <label for="dt">Start Date</label>
                       <input id="dt" name="dt" type="date" />
                   </div>
               
               
                   <div class="col-md-6">

                       <label for="dt">End Date</label>
                       <input id="edt" name="edt" type="date" />
                   </div>
               </div>
  <br /> <hr />
        
      
        <div class="form-group">
            <div class="col-md-offset-4 col-md-10">
                <input type="submit" value="View History" class="btn btn-default" />
            </div>
        </div>
        
    }
</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
